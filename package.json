{
  "name": "vite-vue-starter",
  "version": "1.0.0",
  "keywords": [
    "vite",
    "template",
    "vue-template",
    "vue3",
    "ts",
    "tailwindcss",
    "element-plus"
  ],
  "description": "A vite template with vue3,ts,tailwindcss,element-plus,vue-router,pinia2",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:eslint": "eslint \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "prettier": "prettier --write .",
    "prepare": "husky install",
    "commit": "cz",
    "reinstall": "rimraf yarn.lock && rimraf yarn-error.log && rimraf package.lock.json && rimraf node_modules && yarn dev"
  },
  "dependencies": {
    "@iconify/iconify": "^2.2.1",
    "axios": "^0.27.2",
    "element-plus": "2.2.2",
    "mockjs": "^1.1.0",
    "pinia": "^2.0.14",
    "sass": "^1.52.3",
    "vue": "^3.2.25",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@iconify/json": "^2.1.58",
    "@types/node": "^17.0.40",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/compiler-sfc": "^3.2.36",
    "autoprefixer": "^10.4.7",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.17.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.1.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.0",
    "postcss": "^8.4.14",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.75.6",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.7.3",
    "unplugin-auto-import": "^0.8.6",
    "unplugin-vue-components": "^0.19.6",
    "vite": "^2.9.9",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-purge-icons": "^0.8.1",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vue-tsc": "0.35.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
